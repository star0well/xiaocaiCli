import request, { dowloadReq } from "@/service/index";

export const <%= data.lowerName %>Add = (data) => {
  return request.post({
    url: "/<%= data.lowerName %>/add",
    data,
    sucessTips: "新增成功",
    showReault: true,
    showLoading: true,
  });
};
export const <%= data.lowerName %>Delete = (ids) => {
  return request.delete({
    url: "/<%= data.lowerName %>/delete",
    data: ids,
  });
};
export const <%= data.lowerName %>Patch = (data) => {
  return request.post({
    url: "/<%= data.lowerName %>/edit",
    data,
  });
};

export const <%= data.lowerName %>GetList = (data) => {
  return request.get({
    url: "/<%= data.lowerName %>/list",
    params: data,
    showLoading: true,
  });
};
export const <%= data.lowerName %>Export = (data) => {
  return dowloadReq.post({
    url: "/<%= data.lowerName %>/export",
    params: data,
    responseType: "blob",
  });
};

export const <%= data.lowerName %>GetDetail = (data) => {
  return request.get({
    url: "/<%= data.lowerName %>/get",
    params: data,
  });
};
